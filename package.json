{
  "name": "music-manager",
  "description": "Music Manager",
  "author": {
    "name": "Alex Taylor Danko",
    "email": "alexdankoxxl@gmail.com",
    "url": "https://www.linkedin.com/in/dankozz1/"
  },
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "start": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "test:vitest": "vitest run --config vitest.config.ts",
    "test:playwright": "playwright test --config playwright.config.ts",
    "analyze": "npm run build && npx serve dist",
    "analyze:size": "npm run build && npx vite-bundle-analyzer dist",
    "build:report": "npm run build && open dist/bundle-report.html"
  },
  "dependencies": {
    "@apollo/client": "3.13.8",
    "@mobily/ts-belt": "3.13.1",
    "@vue/apollo-composable": "4.2.2",
    "axios": "1.9.0",
    "graphql": "16.11.0",
    "pinia": "3.0.3",
    "vue": "3.5.16",
    "vue-router": "4.5.1",
    "vuetify": "3.8.9",
    "zod": "3.25.64"
  },
  "devDependencies": {
    "@eslint/js": "9.29.0",
    "@mdi/font": "7.4.47",
    "@playwright/experimental-ct-vue": "1.53.1",
    "@playwright/test": "1.53.1",
    "@rushstack/eslint-patch": "1.8.0",
    "@stylistic/eslint-plugin-ts": "2.11.0",
    "@tsconfig/node20": "20.1.4",
    "@types/node": "24.0.1",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vue/test-utils": "2.4.4",
    "@vue/tsconfig": "0.5.1",
    "@vueuse/core": "10.7.2",
    "autoprefixer": "10.4.20",
    "eslint": "9.29.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-tailwindcss": "3.18.0",
    "eslint-plugin-vue": "10.2.0",
    "jsdom": "24.1.3",
    "npm-run-all2": "6.2.0",
    "prettier": "3.2.4",
    "rollup-plugin-visualizer": "6.0.3",
    "sass": "1.70.0",
    "sass-embedded": "1.77.8",
    "terser": "5.43.1",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.2.3",
    "vue-eslint-parser": "10.1.3",
    "vue-tsc": "2.1.10"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-arm64-gnu": "4.44.1"
  },
  "engines": {
    "node": ">=20.12.0",
    "npm": ">=10.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
